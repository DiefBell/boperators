{
	"name": "boperators",
	"version": "0.1.2",
	"license": "MIT",
	"description": "Operator overloading for TypeScript.",
	"repository": {
		"type": "git",
		"url": "https://github.com/DiefBell/boperators",
		"directory": "package"
	},
	"homepage": "https://github.com/DiefBell/boperators/tree/main/package",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/esm/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/esm/index.d.ts",
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js"
		}
	},
	"keywords": [
		"boperators",
		"typescript",
		"operator",
		"overload",
		"operator-overloading",
		"ts-morph",
		"ast",
		"transform"
	],
	"scripts": {
		"build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
		"watch": "concurrently \"tsc -p tsconfig.cjs.json -w\" \"tsc -p tsconfig.esm.json -w\"",
		"prepare": "bun run build",
		"prepublish": "bun run build"
	},
	"dependencies": {
		"ts-morph": "^27.0.0"
	},
	"peerDependencies": {
		"typescript": ">=5.0.0 <5.10.0"
	},
	"files": [
		"package.json",
		"license.txt",
		"README.md",
		"dist"
	],
	"devDependencies": {
		"@types/node": "^22.0.0",
		"concurrently": "^9.2.1"
	}
}
